# VERSION: set full version, use "devel" for development version
  case $PROJECT in
    S805)
      LIBREELEC_VERSION="7.0.2.7"
      ;;
    S812|X8HP)
      LIBREELEC_VERSION="8.0.0"
      ;;
    S905)
      LIBREELEC_VERSION="8.0.0"
      ;;
    *)
      LIBREELEC_VERSION="devel"
      ;;
  esac

