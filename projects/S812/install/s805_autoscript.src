setenv bootrootfs " boot=LABEL=BOOT disk=LABEL=DISK BOOT_IMAGE=kernel.img "
setenv le_usb "if fatload usb 0 ${loadaddr} kernel.img; then setenv cecconfig cecf; setenv bootargs ${bootrootfs} ${bootargs}; bootm ${loadaddr}; fi;"
setenv le_sd "if fatload mmc 0 ${loadaddr} kernel.img; then setenv cecconfig cecf; setenv bootargs ${bootrootfs} ${bootargs}; bootm ${loadaddr}; fi;"
if test ${bootfromnand} = 1; then setenv bootfromnand 0; saveenv; else run le_usb; run le_sd; fi; run storeboot;
